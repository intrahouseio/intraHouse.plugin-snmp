{
  "id": "snmp",
  "description": "SNMP plugin",
  "gendevices": 0,
  "smart": 1,
  "restartOnChannelsChange":1,
  "single": 0,
  "module": "index.js",
  "params": [],
  "genrules": {},
  "commands": [],
  "sch": {
    "parent": true,
    "scheme": {
      "tools": { "filter": false, "fixed": false, "name": "", "order": -1, "type": "parent", "width": 30 },
      "host": { "filter": true, "fixed": false, "name": "$Url", "order": 1, "type": "STRING", "width": 250 }
    },
    "record": {
      "parent": {
        "name": "$Parent",
        "order": 0,
        "toolbar": false,
        "properties": {
          "host": { "name": "$Host", "order": 0, "type": "STRING" },
          "port": { "name": "$Port", "order": 1, "type": "NUMBER" },
          "community": { "name": "$Community", "order": 2, "type": "STRING" },
          "transport": { "name": "$Transport", "order": 3,  "type": "autocomplete",
            "data": [
              { "id": "udp4", "name": "UDP4" },
              { "id": "udp6", "name": "UDP6" }
            ]
          },
          "version": { "name": "$Version", "order": 4,  "type": "autocomplete",
            "data": [
              { "id": "snmp.Version1", "name": "VERSION1" },
              { "id": "snmp.Version2c", "name": "VERSION2C" }
            ]
          }
        }
      },
      "child": {
        "name": "$Child",
        "order": 1,
        "toolbar": false,
        "properties": {
          "type": { "name": "$Type", "order": 1,  "type": "autocomplete",
            "data": [
              { "id": "get", "name": "GET" },
              { "id": "table", "name": "TABLE" },
              { "id": "trap", "name": "TRAP" }
            ]
          },
          "oid": { "name": "$Oid", "order": 2, "type": "STRING" },
          "trap-oid": { "name": "$TrapOid", "order": 3, "type": "STRING" },
          "interval": { "name": "$Interval", "order": 4, "type": "NUMBER" },
          "dn": { "name": "$Device", "order": 5, "type": "autocomplete", "data": "devicesdroplist" },
        }
      }
    },
    "default": {
      "host": "127.0.0.1",
      "port": 161,
      "community": "public",
      "transport": "udp4",
      "version": "snmp.Version1"
    },
    "defaultchild": {
      "type": "get",
      "oid": "1.3.6.1.2.1.1.5.0",
      "trap-oid": "",
      "interval": 60,
      "dn": ""
    }
  }
}
